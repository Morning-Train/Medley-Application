{
    "name": "morningmedley/application",
    "description": "An Illuminate IoC Container for Morningmedley.",
    "keywords": [
        "morningmedley",
        "wordpress",
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Munk",
            "email": "mh@morningtrain.dk"
        }
    ],
    "autoload": {
        "psr-4": {
            "MorningMedley\\Application\\": "src/MorningMedley/",
            "Illuminate\\Foundation\\": "src/Foundation/"
        },
        "classmap": [
            "src/MorningMedley.php"
        ],
        "files": [
            "src/Foundation/helpers.php"
        ]
    },
    "require": {
        "php": "^8.2",
        "illuminate/container": "^12.31.0",
        "illuminate/support": "^12.31.0",
        "illuminate/config": "^12.3.0",
        "illuminate/cache": "^12.31.0",
        "illuminate/http": "^12.31.0",
        "illuminate/filesystem": "^12.31.0",
        "illuminate/log": "^12.31.0",
        "illuminate/console": "^12.31.0",
        "vlucas/phpdotenv": "^5.6.1",
        "symfony/http-foundation": "^6|^7",
        "symfony/finder": "^6|^7",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "bin": [
        "bin/medley-setup"
    ]
}
